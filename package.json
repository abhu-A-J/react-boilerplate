{
  "name": "react-from-scratch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "build:prod": "env NODE_ENV=production webpack",
    "lint": "eslint src/**/*.{js,jsx}",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
